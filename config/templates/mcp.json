{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp", "--api-key", "{{getenv "CONTEXT7_API_KEY"}}"]
    },
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest",
        "--headless",
        "--browser=chromium",
        "--caps=vision,pdf",
        "--timeout-action=30000",
        "--viewport-size=1920x1080",
        "--isolated"
      ]
    }
  }
}
